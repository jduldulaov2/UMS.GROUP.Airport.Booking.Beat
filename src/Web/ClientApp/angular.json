{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "UMS.GROUP.Airport.Booking.Web": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "progress": false,
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "allowedCommonJsDependencies": [
              "oidc-client"
            ],
            "assets": [
              "src/assets"
            ],
            "styles": [
              "src/assets/reservation/client/assets/vendor/animate.css/animate.min.css",
              "src/assets/reservation/client/assets/vendor/bootstrap-icons/bootstrap-icons.css",
              "src/assets/reservation/client/assets/vendor/boxicons/css/boxicons.min.css",
              "src/assets/reservation/client/assets/vendor/glightbox/css/glightbox.min.css",
              "src/assets/reservation/client/assets/vendor/swiper/swiper-bundle.min.css",
              "src/assets/reservation/client/CustomMenu/css/animate.css",
              //"src/assets/reservation/client/CustomMenu/css/owl.carousel.min.css",
              "src/assets/reservation/client/CustomMenu/css/owl.theme.default.min.css",
              "src/assets/reservation/client/CustomMenu/css/magnific-popup.css",
              "src/assets/reservation/client/CustomMenu/css/aos.css",
              "src/assets/reservation/client/CustomMenu/css/ionicons.min.css",
              "src/assets/reservation/client/CustomMenu/css/flaticon.css",
              "src/assets/reservation/client/CustomMenu/css/icomoon.css",
              "src/assets/reservation/client/assets/css/style.css",
              "src/assets/reservation/client/CartDetails/css/elegant-icons.css",
              "src/assets/reservation/client/CartDetails/css/nice-select.css",
              "src/assets/reservation/client/CartDetails/css/slicknav.min.css",
              "src/assets/reservation/client/CartDetails/css/style.css",
              "src/assets/reservation/admin/plugins/fontawesome-free/css/all.min.css",
              "src/assets/reservation/admin/plugins/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css",
              "src/assets/reservation/admin/plugins/icheck-bootstrap/icheck-bootstrap.min.css",
              "src/assets/reservation/admin/plugins/jqvmap/jqvmap.min.css",
              "src/assets/reservation/admin/dist/css/adminlte.min.css",
              "src/assets/reservation/admin/plugins/overlayScrollbars/css/OverlayScrollbars.min.css",
              "src/assets/reservation/admin/plugins/daterangepicker/daterangepicker.css",
              "src/assets/reservation/admin/plugins/toastr/toastr.min.css",
              "src/assets/reservation/admin/dist/css/adminlte-custom.css",
              "src/assets/reservation/admin/plugins/fullcalendar/main.css",
              "src/assets/reservation/admin/plugins/toastr/toastr.min.css"
            ],
            "scripts": [
              "src/assets/reservation/client/assets/vendor/bootstrap/js/bootstrap.bundle.min.js",
              "src/assets/reservation/client/assets/vendor/glightbox/js/glightbox.min.js",
              "src/assets/reservation/client/assets/vendor/isotope-layout/isotope.pkgd.min.js",
              "src/assets/reservation/client/assets/vendor/swiper/swiper-bundle.min.js",
              "src/assets/reservation/client/assets/vendor/php-email-form/validate.js",
              "src/assets/reservation/client/assets/js/main.js",
              //"src/assets/reservation/client/CustomMenu/js/jquery.min.js",
              //"src/assets/reservation/client/CustomMenu/js/jquery-migrate-3.0.1.min.js",
              "src/assets/reservation/client/CustomMenu/js/popper.min.js",
              //"src/assets/reservation/client/CustomMenu/js/bootstrap.min.js",
              //"src/assets/reservation/client/CustomMenu/js/jquery.easing.1.3.js",
              //"src/assets/reservation/client/CustomMenu/js/jquery.waypoints.min.js",
              //"src/assets/reservation/client/CustomMenu/js/jquery.stellar.min.js",
              //"src/assets/reservation/client/CustomMenu/js/owl.carousel.min.js",
              //"src/assets/reservation/client/CustomMenu/js/jquery.magnific-popup.min.js",
              //"src/assets/reservation/client/CustomMenu/js/aos.js",
              //"src/assets/reservation/client/CustomMenu/js/jquery.animateNumber.min.js",
              //"src/assets/reservation/client/CustomMenu/js/bootstrap-datepicker.js",
              //"src/assets/reservation/client/CustomMenu/js/jquery.timepicker.min.js",
              //"src/assets/reservation/client/CustomMenu/js/scrollax.min.js",
              //"src/assets/reservation/client/CustomMenu/js/main.js",
              //"src/assets/reservation/client/CartDetails/js/jquery-3.3.1.min.js",
              //"src/assets/reservation/client/CartDetails/js/bootstrap.min.js",
              //"src/assets/reservation/client/CartDetails/js/jquery.nice-select.min.js",
              //"src/assets/reservation/client/CartDetails/js/jquery.slicknav.js",
              //"src/assets/reservation/client/CartDetails/js/mixitup.min.js",
              //"src/assets/reservation/client/CartDetails/js/main.js",
              "src/assets/reservation/admin/plugins/jquery/jquery.min.js",
              "src/assets/reservation/admin/plugins/jquery-ui/jquery-ui.min.js",
              "src/assets/reservation/admin/plugins/bootstrap/js/bootstrap.bundle.min.js",
              "src/assets/reservation/admin/plugins/chart.js/Chart.min.js",
              "src/assets/reservation/admin/plugins/jquery-knob/jquery.knob.min.js",
              "src/assets/reservation/admin/plugins/moment/moment.min.js",
              "src/assets/reservation/admin/plugins/daterangepicker/daterangepicker.js",
              "src/assets/reservation/admin/plugins/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js",
              "src/assets/reservation/admin/plugins/summernote/summernote-bs4.min.js",
              "src/assets/reservation/admin/plugins/overlayScrollbars/js/jquery.overlayScrollbars.min.js",
              "src/assets/reservation/admin/dist/js/adminlte.js",
              "src/assets/reservation/admin/plugins/fullcalendar/main.js",
              "src/assets/reservation/admin/plugins/toastr/toastr.min.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "UMS.GROUP.Airport.Booking.Web:build:production"
            },
            "development": {
              "browserTarget": "UMS.GROUP.Airport.Booking.Web:build:development",
              "proxyConfig": "proxy.conf.js"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "UMS.GROUP.Airport.Booking.Web:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist-server",
            "main": "src/main.ts",
            "tsConfig": "tsconfig.server.json"
          },
          "configurations": {
            "dev": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": true
            },
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false
            }
          }
        }
      }
    }
  },
  "defaultProject": "UMS.GROUP.Airport.Booking.Web",
  "cli": {
    "analytics": false
  }
}
